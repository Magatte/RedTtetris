{
	"name": "redttetris",
	"version": "1.0.0",
	"description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
	"scripts": {
		"build-serveur": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
		"start-serveur": "npm run build && nodemon dist/index.js",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "nyc mocha --recursive --compilers js:@babel/register",
		"eject": "react-scripts eject",
		"docs:build": "documentation build components redux -f html -o docs"
	},
	"dependencies": {
		"express": "^4.15.2",
		"history": "^4.9.0",
		"lodash": "^4.17.11",
		"nodemon": "^1.18.9",
		"react": "^16.5.2",
		"react-awesome-button": "^5.0.0",
		"react-dom": "^16.5.2",
		"react-pure-lifecycle": "^3.0.0",
		"react-router-dom": "^5.0.0",
		"react-scripts": "1.1.5",
		"react-sound": "^1.2.0",
		"redux-thunk": "^2.3.0",
		"socket.io": "^2.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@babel/preset-react": "^7.0.0",
		"babel-cli": "^6.26.0",
		"babel-loader": "^8.0.5",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-register": "^6.26.0",
		"mocha": "^5.2.0",
		"nyc": "^13.3.0",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"rimraf": "^2.6.2",
		"socket.io-client": "^2.2.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://Graziellah@github.com/Magatte/RedTtetris.git"
	},
